{
  "name": "sequence-viewer-typescript",
  "version": "1.0.39",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rfv dist && tsc",
